{
  "name": "tribes.ai",
  "displayName": "Tribes.AI",
  "version": "1.1.2",
  "description": "Smart Time Tracking That Drives Revenue",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:web": "vite",
    "build": "run-s clear build:*",
    "build:prepare": "esno scripts/prepare.ts",
    "build:dev": "vite build --mode development && rollup -c --environment NODE_ENV:development -w",
    "build:sta": "vite build --mode staging && rollup -c --environment NODE_ENV:staging",
    "build:prod": "vite build --mode production && rollup -c --environment NODE_ENV:production",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension.zip && jszip-cli add extension -o ./extension.zip",
    "pack:crx": "crx pack extension -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./extension --filename extension.xpi --overwrite-dest",
    "start:chromium": "web-ext run --source-dir ./extension --target=chromium",
    "start:firefox": "web-ext run --source-dir ./extension --target=firefox-desktop",
    "clear": "rimraf extension/dist extension/manifest.json extension.*",
    "lint": "eslint 'src/**/*.{json,ts,js,vue}'",
    "analysis": "windicss-analysis"
  },
  "devDependencies": {
    "@ffflorian/jszip-cli": "^3.1.5",
    "@heroicons/vue": "^1.0.5",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.10.2",
    "@types/webextension-polyfill": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.2.19",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vueuse/core": "^7.5.1",
    "axios": "^0.26.1",
    "chokidar": "^3.5.2",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.3.0",
    "esno": "^0.10.0",
    "fs-extra": "^10.0.0",
    "kolorist": "^1.5.0",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-dotenv": "^0.3.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "tldts": "^5.7.66",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "unplugin-auto-import": "^0.4.8",
    "unplugin-vue-components": "^0.15.4",
    "vite": "^2.9.4",
    "vite-plugin-windicss": "^1.7.1",
    "vue": "^3.2.19",
    "vue-demi": "^0.11.4",
    "web-ext": "^6.4.0",
    "webext-bridge": "^5.0.0",
    "webextension-polyfill": "^0.9.0",
    "windicss": "^3.5.1",
    "windicss-analysis": "^0.3.5"
  }
}
